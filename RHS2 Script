--RHS2 Script

getgenv().job1 = false
getgenv().job2 = false
getgenv().job3 = false
getgenv().star = false
getgenv().team = false
getgenv().gift = false
getgenv().tools = false
getgenv().role = false
getgenv().skin = false
getgenv().coin = false
getgenv().rolex = false
getgenv().potion1 = true
getgenv().potion2 = true
getgenv().potion3 = true
getgenv().potion4 = true
getgenv().potion5 = true
getgenv().potion6 = true
getgenv().potion7 = true


local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("X RHS2 X")

local b = w:CreateFolder("Servant Of The Void")

local a = w:CreateFolder("Sunblox Delivery")

local e = w:CreateFolder("Club Red Potioneer")

local c = w:CreateFolder("Misc")

local d = w:CreateFolder("Other")

e:Toggle("BloxyCola",function(bool)
    getgenv().potion1 = bool
    print(shared.toggle)
    if bool then
        potion1();
    end
end)

e:Toggle("WitchesBrew",function(bool)
    getgenv().potion2 = bool
    print(shared.toggle)
    if bool then
        potion2();
    end
end)

e:Toggle("GhastlyBrew",function(bool)
    getgenv().potion3 = bool
    print(shared.toggle)
    if bool then
        potion3();
    end
end)

e:Toggle("RedSpice",function(bool)
    getgenv().potion4 = bool
    print(shared.toggle)
    if bool then
        potion4();
    end
end)

e:Toggle("GreenSpice",function(bool)
    getgenv().potion5 = bool
    print(shared.toggle)
    if bool then
        potion5();
    end
end)

e:Toggle("DragonFire",function(bool)
    getgenv().potion6 = bool
    print(shared.toggle)
    if bool then
        potion6();
    end
end)

e:Toggle("GoldenChalice",function(bool)
    getgenv().potion7 = bool
    print(shared.toggle)
    if bool then
        potion7();
    end
end)



c:Toggle("Loop Change Team Roles",function(bool)
    getgenv().team = bool
    print(shared.toggle)
    if bool then
        doTappp();
    end
end)

c:Toggle("Loop Change VIP Role",function(bool)
    getgenv().role = bool
    print(shared.toggle)
    if bool then
       role();
    end
end)

c:Toggle("Loop Change Superstar Role",function(bool)
    getgenv().rolex = bool
    print(shared.toggle)
    if bool then
       rolex();
    end
end)

d:Toggle("Redeem All Gifts",function(bool)
    getgenv().gift = bool
    print(shared.toggle)
    if bool then
        doTapppp();
    end
end)

a:Button("Easy",function()
   

local args = {
    [1] = "SunbloxDelivery",
    [2] = "Easy"
}

game:GetService("ReplicatedStorage").Remotes.Jobs.RequestServerStartJob:FireServer(unpack(args))
    
wait(0.5)  
    
local humanoid = game.Players.LocalPlayer.Character.HumanoidRootPart
taxi = true 
    
while taxi == true do
for i, v in pairs(game.Workspace.LightBeam:GetChildren()) do
if v.Name == "Pos" then
humanoid.CFrame = v.CFrame
wait(5)
end
end
end
end)


