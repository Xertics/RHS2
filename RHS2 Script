--RHS2 Script

getgenv().job = false
getgenv().star = false
getgenv().team = false
getgenv().gift = false
getgenv().tools = false



local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("RHS2") -- Creates the window

local b = w:CreateFolder("Main") -- Creates the folder(U will put here your buttons,etc)

local c = w:CreateFolder("Misc")

c:Toggle("Loop Change Roles",function(bool)
    getgenv().team = bool
    print(shared.toggle)
    if bool then
        doTappp();
    end
end)

b:Toggle("Auto Redeem Gifts",function(bool)
    getgenv().gift = bool
    print(shared.toggle)
    if bool then
        doTapppp();
    end
end)

b:Toggle("Auto Complete Job",function(bool)
    getgenv().job = bool
    print(shared.toggle)
    if bool then
        doTap();
    end
end)

b:Toggle("Auto Redeem Stars",function(bool)
    getgenv().star = bool
    print(shared.toggle)
    if bool then
        doTapp();
    end
end)

c:Toggle("Equip All Tools (backpack)",function(bool)
    getgenv().tools = bool
    print(shared.toggle)
    if bool then
        doTappppp();
    end
end)


c:Label("Credits: Xertics#8797",{
    TextSize = 18; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(17, 17, 17); -- Self Explaining
    
}) 




c:DestroyGui()







function doTappp()
spawn(function()
    if getgenv().team == true then
        while true do


local args = {
    [1] = "Freshman"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))

wait(1.5)

local args = {
    [1] = "Sophomore"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))

wait(1.5)

local args = {
    [1] = "Junior"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))

wait(1.5)

local args = {
    [1] = "Senior"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))

wait(1.5)

local args = {
    [1] = "Student"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))

wait(1.5)

local args = {
    [1] = "Cheerleader"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))

wait(1.5)

local args = {
    [1] = "Athlete"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))

wait(1.5)

local args = {
    [1] = "Kid"
}

game:GetService("ReplicatedStorage").Remotes.Data.ChangeRole:FireServer(unpack(args))



end
end
end)
end

function doTap()
spawn(function()
while wait() do
    if getgenv().job == true then

local args = {
    [1] = "VoidServant",
    [2] = "Medium"
}

game:GetService("ReplicatedStorage").Remotes.Jobs.RequestServerStartJob:FireServer(unpack(args));
while wait() do
local args = {
    [1] = "2C1A29CF-651E-47FE-8E3C-94A19A73F1D2"
}

game:GetService("ReplicatedStorage").Remotes.Jobs.ChefUmbras.SectionCompleted:FireServer(unpack(args));
      end
    end
  end
end)
end

function doTapp()
spawn(function()
    while wait() do
    if getgenv().star == true then

game:GetService("ReplicatedStorage").Remotes.ShootingStars.RequestStarReward:InvokeServer();
game:GetService("ReplicatedStorage").Remotes.ShootingStars.RequestStarReward:InvokeServer();
game:GetService("ReplicatedStorage").Remotes.ShootingStars.RequestStarReward:InvokeServer();


     end
   end
end)
end

function doTapppp()
spawn(function()
while wait() do
    if getgenv().gift == true then

local args = {
    [1] = "1238",
    [2] = 1
}

game:GetService("ReplicatedStorage").Remotes.Items.OpenGiftBox:InvokeServer(unpack(args))

end
end
end)
end

function doTappppp()
spawn(function()
for _, tool in ipairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
    if tool:IsA("Tool") then
         tool.Parent = game:GetService("Players").LocalPlayer.Character -- I didn't use Equip because the Equip function unequips any other tools in your character.
    end
  end
end)
end
